			float4 tex2DNode1 = tex2D( _Dragoneyeteexture, uv_Dragoneyeteexture );
			o.Emission = ( _EmissionColor + tex2DNode1 ).rgb;
			o.Alpha = saturate( ( tex2DNode1.r + 0.5 ) );
		}

		ENDCG
		CGPROGRAM
		#pragma surface surf Unlit alpha:fade keepalpha fullforwardshadows 

